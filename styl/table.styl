vanile = #FFFAF0
brown_h1 = #85200c
brown = #91230D

body
  font-family: 'Roboto', sans-serif
  font-size 18px

.pay_first_screen
    background-image url(../img/Ancient.jpg)
    background-size cover
    background-repeat no-repeat
    background-position center
    background-attachment fixed
    padding-top 3%
    padding-bottom 3%
    height 100%
    .container
      width 70%

  .jumbotron
    overflow hidden
    margin-bottom 1%
    background-color rgba(#fff,.92)
    box-shadow 0 0 5px rgba(#000, .3)
    padding 3%
    h1
      color brown_h1
      text-align center
      font-size 36px
      font-weight 900
      margin-top -1%
      margin-bottom 0
    h2
      font-size 22px
      text-align center
      color brown
      margin-top 10px
      margin-bottom 15px
    h3
      color brown
  ul
    li
      color brown
      span
        color #000
    &:last-child
      li
        color brown_h1
.col-md-6
  ul
    border 2px solid #FF9900
    list-style inside
    padding 20px
    box-shadow 0 0 5px #aaa
    border-radius 5px
.col-md-12
  ul
    border 2px solid brown
    list-style inside
    padding 20px
    box-shadow 0 0 5px #aaa
    border-radius 5px
.dates
  color brown
  width 70%
  padding: 20px 5px 0;
  border-radius: 5px;
  border: 2px solid;
  font-size: 18px;
  text-align center
  margin 0 auto 10px
  span
    display: inline-block;
    border-bottom: 1px dashed;
    font-weight bold
    margin-bottom: 10px;

.pay_table
  padding 30px 0
  .container
    width 80%
.table_container
  width: 80%;
  margin: 0 auto 20px;
  padding: 20px;
  border: 1px solid #eee;
.low_screen 
  background-color #fffaf0
  padding 40px 0
  #with_geshe
    width 80%
    margin 0 auto
    #lead
      color #000
table {
  background-color: transparent;
}
caption {
  padding-top: 8px;
  padding-bottom: 8px;
  color: #777777;
  text-align: left;
}
th h4 {
  text-align: center;
  font-weight normal !important
}
.table {
  max-width: 100%;
  margin 0 auto 20px
  overflow-x auto;
  text-align left
}
.no
  background-color #eee
  color #c00
.table p
  margin-bottom 0
.thead
  border: 1px solid #ddd;
  h4
    margin 15px 0
.deleted
  position relative
  margin-top 30px
  &:before
  &:after
    content ''
    position absolute
    z-index 9
    width 60px
    height 2px
    background-color #c00
    top 12px
    left 58px
  &:before
    transform rotate(11deg)
  &:after
    transform rotate(-11deg)
.to-table
  display block
  width 25%
  margin 10px auto
tbody tr td
thead tr th
  text-align center
  vertical-align middle
  width 25%
tbody tr td:first-child
thead tr th:first-child
  text-align left
#silver
  background-image: linear-gradient(to top, #CACACA, #fcfcfc, #CACACA);
  text-shadow: 1px 1px 0px #fff;
#gold
  background-image: linear-gradient(to top,#A99211,#FFE96B,#A99211);
  text-shadow 1px 1px 1px #fff
#diamond
  background-image: linear-gradient(#FAEBD7,transparent,#FAEBD7);
  text-shadow: 1px 1px 0 #ddd;
.alert
  font-weight 900
  color rgb(204, 65, 37);
  text-align center
  margin 20px auto
.big
  font-size 24px
.yes
  color #449d44
.btn-success
  margin-top 20px
#recomends 
  padding-bottom 0
  .one_line
    margin-bottom 36px
#footer
  background-color #240903
  color #fff
  padding 1% 0 .5%
  font-size 12px
